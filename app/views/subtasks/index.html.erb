<h1> <%= @subtasks.length>0 ? @subtasks[0].task.board.name : "Empty ToDo " %> </h1>
<% @subtasks.each do|subtask| %>
    <hr>
    <h5>Title:</h5>
    <td>
        <%= link_to(subtask.title ,subtask_path(subtask.id))%>    
    </td>
    <h5>Description:</h5>
    <br>
    <%= subtask.description %>      
    <hr>
<%end%>